{
  "pages/custom/templates/components/common/header-new": "<header class=\"header1 fixed-top\" role=\"banner\">\r\n    <button class=\"menu-btn mobileMenu-toggle1\">\r\n        <span class=\"material-symbols-outlined\">\r\n            menu\r\n        </span>\r\n    </button>\r\n    <div class=\"header-logo--wrap\">\r\n        <div class=\"header-logo{{#if theme_settings.logo_size '===' 'original'}} logo-full-size-desktop{{/if}}{{#if theme_settings.logo_size_mobile '===' 'original'}} logo-full-size-mobile{{/if}}\">\r\n            \r\n\r\n            <div class=\"logo-and-toggle\">\r\n                <div class=\"desktopMenu-toggle\">\r\n                    <a href=\"javascript:void(0);\" class=\"mobileMenu-toggle\">\r\n                        <span class=\"mobileMenu-toggleIcon\"></span>\r\n                    </a>\r\n                </div>\r\n                \r\n                {{#if page_type '===' 'default'}}\r\n                    <h1 class=\"logo-wrap\">{{> components/common/store-logo}}</h1>\r\n                {{else}}\r\n                    <div class=\"logo-wrap\">{{> components/common/store-logo}}</div>\r\n                {{/if}}\r\n            </div>\r\n            \r\n            <div class=\"quickSearchWrap\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\r\n                {{> components/common/quick-search}}\r\n            </div>\r\n            <div class=\"navUser-wrap\">\r\n                {{> pages/custom/templates/components/common/navigation-new}}\r\n            </div>\r\n        </div>\r\n    </div>\r\n    \r\n    {{> pages/custom/templates/components/common/navigation-menu-new}}\r\n    \r\n</header>\r\n{{#unless page_type '===' 'default'}}\r\n\r\n\r\n{{#if banners.top}}\r\n<div class=\"banners\" data-banner-location=\"top\">\r\n    {{#each (limit banners.top_metadata 1)}}\r\n    <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\r\n        <div data-event-type=\"promotion-click\">\r\n            {{{this.content}}}\r\n        </div>\r\n    </div>\r\n    {{/each}}\r\n</div>\r\n{{/if}}\r\n\r\n{{/unless}}\r\n\r\n{{{region name=\"header_bottom--global\"}}}\r\n{{{region name=\"header_bottom\"}}}\r\n",
  "components/common/store-logo": "<a href=\"{{urls.home}}\" class=\"header-logo__link\" data-header-logo-link>\r\n    {{#if settings.store_logo.image}}\r\n        {{#if theme_settings.logo_size '===' 'original'}}\r\n        <img class=\"header-logo-image-unknown-size\"\r\n             src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\r\n             alt=\"{{settings.store_logo.title}}\"\r\n             title=\"{{settings.store_logo.title}}\">\r\n        {{else}}\r\n        <div class=\"header-logo-image-container\">\r\n            <img class=\"header-logo-image\"\r\n                 src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\r\n                 srcset=\"{{getImageSrcset1x2x settings.store_logo.image theme_settings.logo_size}}\"\r\n                 alt=\"{{settings.store_logo.title}}\"\r\n                 title=\"{{settings.store_logo.title}}\">\r\n        </div>\r\n        {{/if}}\r\n    {{else}}\r\n    <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n    {{/if}}\r\n</a>\r\n",
  "components/common/quick-search": "<div>\r\n    {{{ snippet \"forms_search\"}}}\r\n    <form class=\"form\" action=\"{{urls.search}}\">\r\n        <fieldset class=\"form-fieldset\">\r\n            <div class=\"form-field\">\r\n                <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\r\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\r\n                <button type=\"submit\" class=\"form-button\">\r\n                    <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg><use xlink:href=\"#icon-search\" /></svg>\r\n                    </i>\r\n                </button>\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\r\n    <p role=\"status\"\r\n       aria-live=\"polite\"\r\n       class=\"aria-description--hidden\"\r\n       data-search-aria-message-predefined-text=\"{{{lang 'search.results.quick_count_live'}}}\"\r\n    ></p>\r\n</div>\r\n",
  "pages/custom/templates/components/common/navigation-menu-new": "\r\n    <div class=\"container\" id=\"navbar_header\">\r\n        <nav class=\"navbar navbar-expand-lg navbar-dark \">\r\n            <div class=\"menu-container\">\r\n                <ul class=\"mega-menu\">\r\n                    {{#each categories}}\r\n                            {{> pages/custom/templates/components/common/navigation-list-new}}\r\n                    {{/each}}\r\n                    {{#if theme_settings.show_content_navigation}}\r\n                        {{#each pages}}\r\n                            {{#if ../theme_settings.navigation_design '===' 'simple'}}\r\n                                {{> components/common/navigation-list-alternate}}\r\n                            {{/if}}\r\n                            {{#if ../theme_settings.navigation_design '===' 'mega'}}\r\n                            \r\n                                {{> pages/custom/templates/components/common/navigation-list-new}}\r\n                            {{/if}}\r\n                        {{/each}}\r\n                    {{/if}}\r\n                </ul>\r\n            </div>\r\n        </nav>\r\n    </div>\r\n\r\n\r\n<script async src=\"{{cdn 'assets/js/menu.js'}}\"></script>\r\n",
  "pages/custom/templates/components/common/navigation-new": "<nav class=\"navUser\">\r\n    <ul class=\"navUser-section navUser-section--alt\">\r\n        <li class=\"navUser-item navUser-item--account item-user\">\r\n            <i class=\"icon\" aria-hidden=\"true\">\r\n                <svg><use xlink:href=\"#icon-user\" /></svg>\r\n            </i>\r\n            {{#if customer}}\r\n                <a class=\"navUser-action\" href=\"{{urls.account.index}}\">{{lang 'common.account'}}</a> or\r\n                <a class=\"navUser-action\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\r\n            {{else}}\r\n                <a class=\"navUser-action\"\r\n                   href=\"{{urls.auth.login}}\"\r\n                   aria-label=\"{{lang 'common.login'}}\"\r\n                >\r\n                    {{lang 'common.login'}}\r\n                </a>\r\n                {{#if settings.account_creation_enabled}}\r\n                    <span>{{lang 'common.or'}}</span> <a class=\"navUser-action\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\r\n                {{/if}}\r\n            {{/if}}\r\n        </li>\r\n        {{> components/common/currency-selector}}\r\n        <li class=\"navUser-item navUser-item--mobile-search item-search\">\r\n            <i class=\"icon\" aria-hidden=\"true\">\r\n                <svg><use xlink:href=\"#icon-search\" /></svg>\r\n            </i>\r\n        </li>\r\n        <li class=\"navUser-item navUser-item--cart item-cart\">\r\n            <a\r\n                class=\"navUser-action\"\r\n                data-cart-preview\r\n                data-dropdown=\"cart-preview-dropdown\"\r\n                data-options=\"align:left\"\r\n                href=\"{{urls.cart}}\"\r\n                aria-label=\"{{lang 'cart.nav_aria_label'}}\"\r\n            >\r\n                <i class=\"icon\" aria-hidden=\"true\">\r\n                    <svg><use xlink:href=\"#icon-cart\" /></svg>\r\n                </i>\r\n                (<span class=\"{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>)\r\n            </a>\r\n\r\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n        </li>\r\n    </ul>\r\n</nav>\r\n",
  "pages/custom/templates/components/common/navigation-list-new": "{{#if children}}\r\n<li class=\"dropdown2\">\r\n    <div>\r\n        <span>{{name}}</span>\r\n        <span class=\"material-symbols-outlined\">\r\n            chevron_right                \r\n        </span>\r\n    </div>\r\n\r\n    <ul class=\"menu\" id=\"menu_{{id}}\" parent=\"{{id}}\">\r\n        <li class=\"sub-dropdown\">\r\n            <ul class=\"sub-menu sub-menu-not-children\" id=\"menuNotChildren_{{id}}\">\r\n\r\n            </ul>\r\n        </li>\r\n        {{#each children}}\r\n            {{#if children}}\r\n            <li class=\"sub-dropdown\">\r\n                <div class=\"\">\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                    {{#if children}}\r\n                    <span class=\"material-symbols-outlined\">\r\n                        chevron_right\r\n                    </span>\r\n                    {{/if}}\r\n                </div>\r\n                <ul class=\"sub-menu\" id=\"submenu_{{id}}\">\r\n                    <li class=\"linkMenuMovile\" id=\"menu_{{id}}\">\r\n                        <a class=\"\" href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n                    {{#if children}}\r\n                        {{#each children}}\r\n                            <li class=\" childrenChildren\" id=\"menu_{{id}}\">\r\n                                <a class=\"\" href=\"{{url}}\">{{name}}</a>\r\n                            </li>\r\n                        {{/each}}\r\n                    {{/if}}\r\n                </ul>\r\n            </li>\r\n            {{else}}\r\n            <li style=\"\" class=\"notChildren\" id=\"menu_{{id}}\">\r\n                <a href=\"{{url}}\">{{name}}</a>\r\n            </li>\r\n            {{/if}}\r\n        {{/each}}\r\n    </ul>\r\n</li>\r\n{{else}}\r\n<li class=\"dropdown2 notChildren2\">\r\n    <div style=\"display: block; clear: both;\">\r\n        <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" aria-label=\"{{name}}\">\r\n             {{name}}\r\n         </a>\r\n    </div>\r\n</li>\r\n{{/if}}\r\n",
  "components/common/navigation-list-alternate": "{{#if children}}\r\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\"\r\n       href=\"{{url}}\"\r\n       data-collapsible=\"navPages-{{id}}\"\r\n       aria-label=\"{{name}}\"\r\n    >\r\n        {{name}}\r\n        <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n            <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n        </i>\r\n    </a>\r\n    {{> components/common/navigation-dropdown}}\r\n{{else}}\r\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\r\n       href=\"{{url}}\"\r\n       aria-label=\"{{name}}\"\r\n    >\r\n        {{name}}\r\n    </a>\r\n{{/if}}\r\n",
  "components/common/currency-selector": "{{#if currency_selector.currencies.length '>' 1}}\r\n    <li class=\"navUser-item navUser-item-currency\">\r\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">{{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-downselector-small\" /></svg></i></a>\r\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\r\n            {{#each currency_selector.currencies}}\r\n            <li class=\"dropdown-menu-item\">\r\n                <a href=\"{{{switch_url}}}\"\r\n                   data-currency-code=\"{{{code}}}\"\r\n                   {{#if cart_currency_switch_url.length '>' 0 }}\r\n                   data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\r\n                   {{/if}}\r\n                   data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\r\n                >\r\n                {{#if is_active}}\r\n                    <strong>{{name}}</strong>\r\n                {{else}}\r\n                    {{name}}\r\n                {{/if}}\r\n                </a>\r\n            </li>\r\n            {{/each}}\r\n        </ul>\r\n    </li>\r\n{{/if}}\r\n",
  "components/common/navigation-dropdown": "<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n    <ul class=\"navPage-subMenu-list\">\r\n        {{#each children}}\r\n            <li class=\"navPage-subMenu-item-child\">\r\n                {{#if children}}\r\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\r\n                        <i class=\"icon navPages-secondLevel\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-arrow-right\" /></svg></i>\r\n                        {{name}}\r\n                        <i class=\"icon navPages-action-moreIcon navPages-action-more\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-plus\" /></svg></i>\r\n                        <i class=\"icon navPages-action-moreIcon navPages-action-less\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-minus\" /></svg></i>\r\n                    </a>\r\n                    {{> components/common/navigation-dropdown}}\r\n                {{else}}\r\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">\r\n                        <i class=\"icon navPages-secondLevel\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-arrow-right\" /></svg></i>\r\n                        {{name}}\r\n                    </a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n</div>\r\n"
}
